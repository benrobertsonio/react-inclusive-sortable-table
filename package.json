{
  "name": "react-inclusive-sortable-table",
  "type": "module",
  "version": "1.1.0",
  "description": "An accessible, sortable table based off of Heydon Pickering&#x27;s Sortable Table from Inclusive Components.",
  "keywords": [
    "accessibility",
    "a11y",
    "react",
    "inclusive components",
    "responsive",
    "table"
  ],
  "author": "benjamingrobertson",
  "license": "MIT",
  "repository": "benjamingrobertson/react-inclusive-sortable-table",
  "bugs": "https://github.com/benjamingrobertson/react-inclusive-sortable-table/issues",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "npm run build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
